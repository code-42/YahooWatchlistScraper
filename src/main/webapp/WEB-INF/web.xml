  </welcome-file-list>
  <servlet>
    <servlet-name>StockControllerServlet</servlet-name>
    <servlet-class>net.ed.StockControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockControllerServlet</servlet-name>
    <url-pattern>/YahooWatchlistScraper</url-pattern>
  </servlet-mapping>
</web-app>